  "scripts": {
    "mock-server": "json-server --watch mock/data.json",
    "tsc": "tsc",
    "check-circular-dependency": "npx madge --extensions js,ts,tsx --circular .",
    "update-packages": "npm-check -u",
    "prepare": "patch-package && husky install",
    "test": "jest --colors --config ./jest.config.js",
    "test-ci": "jest --colors --verbose --runInBand --config ./jest.config.js",
    "test-coverage": "jest --colors --config ./jest.config.js --coverage",
    "test-update-snapshots": "jest --colors --updateSnapshot --no-cache",
    "lint": "eslint -c .eslintrc.yml --ext ts,js,tsx,jsx src test webpack --color",
    "lint-fix": "npm run lint -- --fix",
    "dev": "webpack --config webpack/main/webpack-main.dev.js --color",
    "dev-admin": "webpack --config webpack/admin/webpack-admin.dev.js --color",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack/main/webpack-main.dev.js --color",
    "start-admin": "cross-env NODE_ENV=development webpack-dev-server --config webpack/admin/webpack-admin.dev.js --color",
    "build": "webpack --config webpack/main/webpack-main.prod.js --color",
    "build-admin": "webpack --config webpack/admin/webpack-admin.prod.js --color",
    "ci": "npm run lint && npm run tsc -- --noEmit && npm run test-coverage && npm run build && npm run build-admin",
    "prod": "npm run ci"
  },
